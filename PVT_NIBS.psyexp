      <CodeComponent name="ISIcode" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# All the durations are in seconds&amp;#10;minISI = 2&amp;#10;maxISI = 10&amp;#10;&amp;#10;# Task duration (set to 10 minutes = 600 seconds)&amp;#10;length_of_task = 600&amp;#10;&amp;#10;# Feedback duration&amp;#10;feed = 0.5&amp;#10;&amp;#10;# A timer&amp;#10;timing = core.Clock()&amp;#10;&amp;#10;# Loading the beep sound&amp;#10;warning_beep = sound.Sound('beep.wav')&amp;#10;&amp;#10;# Get the subject ID dynamically from the experiment info&amp;#10;subject_id = expInfo['participant']&amp;#10;&amp;#10;# Create the output folder if it doesn't exist&amp;#10;output_folder = 'pvt_txt_data'&amp;#10;os.makedirs(output_folder, exist_ok=True)&amp;#10;&amp;#10;# Extract the subject ID and format the output file name&amp;#10;subject_id = expInfo['participant']&amp;#10;output_filename = f&quot;sub-{subject_id}_PVT_data.txt&quot;&amp;#10;output_path = os.path.join(output_folder, output_filename)&amp;#10;&amp;#10;# Extract the date and time from the 'date' key&amp;#10;date_str = expInfo[&quot;date&quot;].split(&quot;_&quot;)[0]  # e.g., '2024-12-19'&amp;#10;time_str = expInfo[&quot;date&quot;].split(&quot;_&quot;)[1].replace(&quot;h&quot;, &quot;&quot;)[:4]  # e.g., '1601'&amp;#10;&amp;#10;# Create and write the header to the file&amp;#10;with open(output_path, 'w') as file:&amp;#10;    file.write(f&quot;PVT-{subject_id} SINGLE TRIAL DATA FILE\n&quot;)&amp;#10;    file.write(f'&quot;STUDY:&quot;,&quot;PVT_NIBS&quot;,&quot;E. INITIALS:&quot;,&quot;XYZ&quot;,&quot;S. INITIALS:&quot;,&quot;XYZ&quot;\n')&amp;#10;    file.write(f'&quot;S. ID NUMBER:&quot;,&quot;{subject_id}&quot;,&quot;TRIAL NUMBER:&quot;,&quot;12&quot;\n')&amp;#10;    file.write(f'&quot;TRIAL DATE:&quot;,&quot;{date_str}&quot;,&quot;TRIAL TIME:&quot;,&quot;{time_str}&quot;\n')&amp;#10;    file.write(f'&quot;SLEEPY PRE, POST TRIAL:&quot;,&quot;0&quot;,&quot;0&quot;\n')&amp;#10;    file.write(f'&quot;ISI MIN (ms):&quot;,&quot;2000&quot;,&quot;ISI MAX (ms):&quot;,&quot;10000&quot;\n')&amp;#10;    file.write(f'&quot;TRIAL LENGTH (s):&quot;,&quot;600&quot;,&quot;TASK:&quot;,&quot;V&quot;\n')&amp;#10;    file.write(f'&quot;HAND:&quot;,&quot;R&quot;,&quot;PVT S/N:&quot;,&quot;2529&quot;\n')&amp;#10;    file.write(f'&quot;DATA IS RT(ms) &amp; ET(sec):&quot;\n')&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="/* Syntax Error: Fix Python code */" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# Generate a random ISI within the range using a rectangular distribution&amp;#10;randISI = random() * (maxISI - minISI) + minISI&amp;#10;&amp;#10;# If it is the first trial&amp;#10;if PVT_Trials.thisN == 0:&amp;#10;    overall_timer = core.Clock()&amp;#10;    realISI = 0&amp;#10;&amp;#10;# For subsequent trials, include feedback duration in ISI&amp;#10;if PVT_Trials.thisN &gt; 0:&amp;#10;    realISI = feed&amp;#10;&amp;#10;# A message when the participant misses&amp;#10;message = 'You did not hit the button!'&amp;#10;&amp;#10;# Adding the ISI so it is saved in the datafile&amp;#10;thisExp.addData('ISI', randISI)&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Py" valType="str" updates="None" name="Code Type"/>
        <Param val="keys = dontrespond.getKeys(keyList=['space'], waitRelease=False)&amp;#10;keys = [key.name for key in keys]&amp;#10;&amp;#10;# If a key is pressed during the ISI, mark it as a &quot;False Start!&quot;&amp;#10;if &quot;space&quot; in keys:&amp;#10;    message = &quot;False Start!&quot;&amp;#10;    continueRoutine = False&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
